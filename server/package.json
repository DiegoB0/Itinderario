{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "docker-build": "docker build -t mysql-file -f Dockerfile-db .",
    "docker-run": "docker run -d --name mysql-container -p 3306:3306 mysql-file",
    "docker-stop": "docker stop mysql-container",
    "docker-remove": "docker rm mysql-container",
    "docker-api-build": "docker build -t web-api -f Dockerfile-api .",
    "docker-api-run": "docker run -d --name my-api-container -p 3000:3000 web-api",
    "docker-api-stop": "docker stop my-api-container",
    "docker-api-remove": "docker rm my-api-container"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.6",
    "@types/mysql": "^2.15.21",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": "16.20.0",
    "typescript": ">=5.2"
  }
}
